
//draw pattern                             //i=1 * j=1

package java_package;                      //    *

public class java_class6
{
	public static void main(String[]args)
	{
		for(int i=1; i<=10;i++)
		{
			System.out.println("*");
			for(int j=1;j<=i;j++)
			{
				System.out.print("*");
			}
			System.out.print("*");
		}
	}

}
