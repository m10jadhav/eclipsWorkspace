package second_java_project;

public class second
{
	int id;
	String name;
	second(int i, String n)
	{
		this.id =i;
		this.name = n;
	}
	void display()
	{
		System.out.println(id+""+  name);
	}
	public static void main(String args[])
	{
		second s1 = new second (111, "Karan");
		second s2 = new second(222, "Aryan");
		s1.display();
		s2.display();
	}

}
